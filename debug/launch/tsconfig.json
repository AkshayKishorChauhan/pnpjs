{
    "extends": "../../tsconfig.json",
    "compilerOptions": {
        "target": "esnext",
        "module": "commonjs",
        "composite": false,
        "baseUrl": ".",
        "rootDir": "../../",
        "outDir": "../../build/debugging",
        "incremental": true,
        "noUnusedLocals": false,
        "allowUnreachableCode": true,
        "allowUnusedLabels": true,
        "esModuleInterop": true,
        "types": [
            "sharepoint"
        ],
        "lib": [
            "dom",
            "es2015"
        ],
        "paths": {
            "@pnp/*": [
                "../../packages/*"
            ]
        }
    },
    "files": [
        "./main.ts"
    ],
    "exclude": [
        "../../packages/graph/*"
    ]
}